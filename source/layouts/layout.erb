<!doctype html>
<html>
  <head>
    <meta charset="utf-8">

    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "The Middleman" %></title>

    <%= stylesheet_link_tag "normalize", "all" %>
  </head>

  <body class="<%= page_classes %>">

    <header class="banner navbar navbar-default navbar-static-top" role="banner">
      <div class="container">
        <div class="navbar-header">
          <a class="navbar-brand" href="http://www.watsonswander.com/">WatsonsWander</a>
        </div>

        <nav class="" role="navigation">
          <ul id="menu-primary-navigation" class="nav navbar-nav"><li class="menu-who"><a href="http://www.watsonswander.com/who/">Who</a></li>
            <li class="menu-what"><a href="http://www.watsonswander.com/what/">What</a></li>
            <li class="menu-where"><a href="http://www.watsonswander.com/where/">Where</a></li>
            <li class="menu-why"><a href="http://www.watsonswander.com/why/">Why</a></li>
          </ul>
        </nav>
      </div>
    </header>

    <div class='container'>
      <%= yield %>
    </div>

    <footer class="container">

    </footer>

  </body>
  <%= javascript_include_tag  "all" %>
</html>
